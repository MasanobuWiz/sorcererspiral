    function makePencilBanner(data){
	  if (typeof data === "undefined")
		return;
      $("#globalpromo1").css({
        "background" : data.backgroundColor,
        "color" : data.secondaryColor
      });
      $(".pencil-banner-copy").text(data.copy);
      $(".pencil-banner-discount").text(data.discount);
      $(".pencil-banner-code").text(data.code)
      $(".pencil-banner-primary-color").css("color", data.primaryColor);
      $(".pencil-banner-link").attr("href", "http://www.cafepress.com/m/pencilbanner/popup/us" + data.code.toLowerCase());
      $(".ribbon").removeClass("pencil-banner-hidden");
    }

    function replaceAll(str, find, replace) {
      return str.replace(new RegExp(find, 'g'), replace);
    }

    function makeHeroBanner(banner, type){
      var heroTemplate = {
        "desktop": "<div class='hero-banner-wrap' style='background-color:{{color}};'> <a href='{{url}}' title='{{caption}}'> <img alt='{{caption}}' width='1000' height='266' src='{{image}}'/></a> </div>",
        "mobile": "<a href='{{url}}'><img alt='{{caption}}' src='{{image}}'></a>",
      };
      var html = replaceAll(heroTemplate[type], "{{url}}", banner.url);
          html = replaceAll(html, "{{color}}", banner.color);
          html = replaceAll(html, "{{caption}}", banner.caption);
          html = replaceAll(html, "{{image}}", banner.image[type]);
      return html;
    }

     function addHerobanners(location, bannerSet, heroes, type){
      if (bannerSet){
        var html = "";
        for(var i = 0; i < bannerSet.heroes.length; i++){
          html += makeHeroBanner(heroes[bannerSet.heroes[i]], type);
        }
        $(location).empty();
        $(location).append(html);
         if(type === "mobile"){
          $("[data-heroBanners='mobile']").css('width', '1924px');
          $("[data-heroBanners='mobile'] img").css('display','block');
         }
      }

    }


    function pickData (data, currentTime){
      for(i = 0; i < data.length; i++){
        if ( currentTime >= data[i].startTime
          && currentTime < data[i].endTime ){
            return data[i];
          }
        }
      }

    function getCurrentTime(){
      var req = new XMLHttpRequest();
      req.onload = function (e){
          if (req.readyState === 4) {
              if (req.status === 200) {
                var now = Date.parse(req.getResponseHeader('Date'))/1000;
                makePencilBanner(pickData(promos, now));
              }
          }
      };
      req.open('GET', window.location.protocol + '//' + window.location.hostname + '/content/global/img/blank.gif?_=' + new Date().getTime(), true);
      req.send(null);
    }

		masonryURL = 'http://content.cpcache.com/mobile2.0/js/vendor/jquery.masonry.pkgd.4.1.1.js';
		$.getScript('http://content.cpcache.com/minify/js/global-nav-h.1.01.min.js');

    getCurrentTime();


    }
  ) (jQuery);
}

(function(){
	var checkjQueryInterval = setInterval(checkjQueryPresent, 100);
	function checkjQueryPresent(){
		if (window.jQuery){
			clearInterval(checkjQueryInterval);
			afterLoadHeader();
		}
	}
})();
</script>

