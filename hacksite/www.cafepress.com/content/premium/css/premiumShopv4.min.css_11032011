tr, td, table, form, input, button, select, option, textarea{font-size:12px; page-break-inside:avoid}
table.border{color:black; page-break-inside:avoid; border-color:Black; border-width:1px; border-style:solid; border-collapse:collapse}
.tableheader{background-color:#9c3}
.smallprint, .footer{font-size:11px}
.subhead{font-size:12px; color:#9C0; font-weight:bold}
.head, H1{font-size:17px; color:#000; line-height:100%}
hr{border:0; color:#ccc; background-color:#ccc; height:1px; width:100%; text-align:left}
img{border:0}
img.imageborder{border-width:0}
.clear{clear:both}
.tableheaderbg{background-color:#000; color:#fff; text-align:left}
.checkoutbg{background-color:#fff; text-align:left}
.tablebg{background-color:#F5F5F5; color:#000}
.tablebg2{background-color:#E9E9E9; color:#000}
.sidebartext{color:#000}
.sidebarbg{background-color:#fff}
.alertmessage{color:#F00}
.ffffff, .ffffff:link, .ffffff:visited{color:#fff}
.75pt{font-size:7.5pt}
.8pt{font-size:8pt}
.arial8pt{font-size:8pt; font-family:arial,helvetica,sans-serif}
.85pt{font:8.5pt}
ul.horizontal{list-style-type:none}
ul.horizontal li{display:inline}
/* begin cmiller CSS additions */
.standardText{font-size:12px}
.smallText, .mediumprint, .textmedium{font-size:11px}
.xsmallText, .smallprint, .smallerprint, .textsmall, .tinyprint{font-size:10px}
.standardHeading{font-weight:bold}
.mediumHeading{font-size:14px; font-weight:bold}
.smallHeading{font-size:11px; font-weight:bold}
.xsmallHeading{font-size:10px; font-weight:bold}
.largeHeading{font-size:15px; font-weight:bold}
.xlargeHeading{font-size:17px}
.xxlargeHeading{font-size:19px}
.standardSectionHeader{background-color:#999; padding:4px; font-size:14px; font-weight:bold; color:#FFF}
.largeSectionHeader{background-color:#999; padding:4px; font-size:14px; font-weight:bold; color:#FFF}
.smallSectionHeader{background-color:#999; padding:4px; font-size:11px; font-weight:bold; color:#FFF}
.standardTableHeader{background-color:#999; font-weight:bold; color:#FFF}
.largeTableHeader{background-color:#999; font-size:14px; font-weight:bold; color:#FFF; vertical-align:middle}
.smallTableHeader{background-color:#999; padding:4px 0px; font-size:11px; font-weight:bold; color:#FFF; vertical-align:middle}
.buttonPrimary{background-color:#F60; border-style:outset; border-width:2px; padding:1px 6px; line-height:16px; text-align:center; font-family:verdana; font-weight:bold; font-size:10px; color:#FFF; text-decoration:none}
.buttonPrimary:link{color:#FFF}
.buttonPrimary:visited{color:#FFF}
.buttonPrimary:hover{background-color:#FFB448; color:#FFF; text-decoration:none}
.buttonPrimaryMedium{background-color:#F60; border-style:outset; border-width:2px; padding:3px 8px; line-height:21px; text-align:center; font-family:verdana; font-weight:bold; font-size:11px; color:#FFF; text-decoration:none}
.buttonPrimaryMedium:link{color:#FFF}
.buttonPrimaryMedium:visited{color:#FFF}
.buttonPrimaryMedium:hover{background-color:#FFB448; color:#FFF; text-decoration:none}
.buttonSecondary{background-color:#F90; border-style:outset; border-width:2px; padding:1px 6px; line-height:16px; text-align:center; font-family:verdana; font-weight:bold; font-size:10px; color:#FFF; text-decoration:none}
.buttonSecondary:link{color:#FFF}
.buttonSecondary:visited{color:#FFF}
.buttonSecondary:hover{background-color:#FFB448; color:#FFF; text-decoration:none}
.buttonGray{background-color:#888; border-style:outset; border-width:2px; padding:1px 6px; line-height:16px; text-align:center; font-family:verdana; font-weight:bold; font-size:10px; color:#FFF; text-decoration:none}
.buttonGray:link{color:#FFF}
.buttonGray:visited{color:#FFF}
.buttonGray:hover{background-color:#AAA; color:#FFF; text-decoration:none}
.buttonGrayMedium{background-color:#888; border-style:outset; border-width:2px; padding:3px 8px; line-height:21px; text-align:center; font-family:verdana; font-weight:bold; font-size:11px; color:#FFF; text-decoration:none}
.buttonGrayMedium:link{color:#FFF}
.buttonGrayMedium:visited{color:#FFF}
.buttonGrayMedium:hover{background-color:#AAA; color:#FFF; text-decoration:none}
.buttonCloseWindow{background-color:#CCC; border-style:outset; border-width:1px; padding:1px 2px; font-weight:bold; font-size:10px; color:#000; float:right; width:94px}
.buttonCloseWindow .x{display:block; border:1px solid #000; float:right}
.buttonCloseWindow a:link{color:#000; text-decoration:none}
.buttonCloseWindow a:visited{color:#000; text-decoration:none}
.buttonCloseWindow a:hover{color:#000; text-decoration:none}
.buttonWrapper{padding:2px 0px}
.errorMessageWrapper{color:#F00; font-weight:bold; margin:4px 0px}

