#intlCurrencyDropdown li a{padding-right:0}
.aud, .cad, .gbp, .usd, .eur{padding-left:23px; position:relative; display:block}
.aud{background:url(http://content.cpcache.com/global/img/intl_flagAUS.gif) left 1px no-repeat}
.cad{background:url(http://content.cpcache.com/global/img/intl_flagCAN.gif) left 1px no-repeat}
.gbp{background:url(http://content.cpcache.com/global/img/intl_flagUK.gif) left 1px no-repeat}
.usd{background:url(http://content.cpcache.com/global/img/intl_flagUS.gif) left 1px no-repeat}
.eur{background:url(http://content.cpcache.com/global/img/intl_flagEuro.gif) left 1px no-repeat}
#intlCurrencyDropdown .aud, #intlCurrencyDropdown .cad, #intlCurrencyDropdown .gbp, #intlCurrencyDropdown .usd, #intlCurrencyDropdown .eur{background-position:left center}

