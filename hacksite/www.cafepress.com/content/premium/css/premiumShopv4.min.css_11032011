.bodyText{font-size:12px}
.storesmallprint{font-size:10px}

/* Element Default Styling 
---------------------------------- */
.sidebarbg{border-right:1px solid #000}
.breadCrumb{margin:0 0 20px 0; font-size:10px}

/* Search Box styles
---------------------------------- */
